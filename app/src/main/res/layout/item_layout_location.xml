<?xml version="1.0" encoding="utf-8"?>


<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="160dp"
    android:layout_margin="16dp"
    android:elevation="8dp"
    app:cardCornerRadius="8dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/tv_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Living" />

        <ImageView
            android:id="@+id/iv_1"
            android:visibility="invisible"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:layout_constraintEnd_toStartOf="@+id/iv_2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/iv_2"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_2"
            android:visibility="invisible"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:layout_constraintEnd_toStartOf="@+id/iv_3"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_1"
            app:layout_constraintTop_toTopOf="@+id/iv_3"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_3"
            android:visibility="invisible"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:layout_constraintEnd_toStartOf="@+id/iv_4"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_2"
            app:layout_constraintTop_toTopOf="@+id/iv_4"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_4"
            android:layout_width="24dp"
            android:visibility="invisible"
            android:layout_height="24dp"
            app:layout_constraintEnd_toStartOf="@+id/iv_5"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_3"
            app:layout_constraintTop_toTopOf="@+id/iv_5"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_5"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:visibility="invisible"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_4"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_6"
            android:layout_width="24dp"
            android:visibility="invisible"
            android:layout_height="24dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_7"
            app:layout_constraintEnd_toStartOf="@+id/iv_7"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_9"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_7"
            android:visibility="invisible"
            android:layout_width="24dp"
            android:layout_height="24dp"
            app:layout_constraintBottom_toBottomOf="@+id/iv_10"
            app:layout_constraintEnd_toStartOf="@+id/iv_8"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_6"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_8"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@+id/iv_10"
            app:layout_constraintEnd_toStartOf="@+id/iv_10"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_7"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_9"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="@+id/iv_6"
            app:layout_constraintEnd_toStartOf="@+id/iv_6"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintHorizontal_chainStyle="spread"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

        <ImageView
            android:id="@+id/iv_10"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:visibility="invisible"
            android:layout_margin="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iv_8"
            app:srcCompat="@drawable/ic_baseline_location_searching_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>

